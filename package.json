{
  "name": "lovue",
  "version": "3.0.1",
  "description": "Love Vue: UI components for Vue",
  "module": "dist/lovue.esm.js",
  "unpkg": "dist/lovue.min.js",
  "jsdelivr": "dist/lovue.min.js",
  "repository": {
    "url": "https://github.com/loliconer/lovue.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/loliconer/lovue/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "serve": "rollup -c --watch",
    "serveExtension": "rollup -c rollup.extension.config.js --watch",
    "normalize": "lessc src/less/normalize.less dist/normalize.css",
    "minifyNormalize": "cleancss -o dist/normalize.min.css dist/normalize.css",
    "less": "lessc src/less/lovue.less dist/lovue.css",
    "minifyLess": "cleancss -o dist/lovue.min.css dist/lovue.css",
    "lessToDoc": "lessc src/less/lovue.less doc/css/lovue.css",
    "esm": "rollup -c --environment production,esm",
    "js": "rollup -c --environment production",
    "min": "terser dist/lovue.js -o dist/lovue.min.js -c",
    "buildLovue": "npm run less && npm run minifyLess && npm run lessToDoc && npm run normalize && npm run minifyNormalize && npm run esm && npm run js && npm run min",
    "lessExtension": "lessc src/less/lovue.extension.less dist/lovue.extension.css",
    "minifyLessExtension": "cleancss -o dist/lovue.extension.min.css dist/lovue.extension.css",
    "lessExtensionToDoc": "lessc src/less/lovue.extension.less doc/css/lovue.extension.css",
    "esmExtension": "rollup -c rollup.extension.config.js --environment production,esm",
    "jsExtension": "rollup -c rollup.extension.config.js --environment production",
    "minExtension": "terser dist/lovue.extension.js -o dist/lovue.extension.min.js -c",
    "buildExtension": "npm run lessExtension && npm run minifyLessExtension && npm run lessExtensionToDoc && npm run esmExtension && npm run jsExtension && npm run minExtension",
    "cjsUtils": "rollup src/utils.js --o dist/utils.common.js --f cjs --no-strict --preferConst --exports named",
    "esmUtils": "rollup src/utils.js --o dist/utils.esm.js --f esm --no-strict --preferConst",
    "jsUtils": "rollup src/utils.js --o dist/utils.js --f iife -n utils --no-strict --exports named",
    "minUtils": "terser dist/utils.js -o dist/utils.min.js -c",
    "buildUtils": "npm run esmUtils && npm run jsUtils && npm run minUtils",
    "build": "npm run buildLovue && npm run buildExtension && npm run buildUtils",
    "test": "jest"
  },
  "author": "loliconer",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.9.0",
    "qrcode.vue": "^1.6.1",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-vue": "^5.0.0",
    "terser": "^4.0.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "ws": "^7.0.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0",
    "not ie_mob > 0"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(css|less)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(vue-awesome)/)"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
